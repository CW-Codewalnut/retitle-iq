	"name": "seo-ai-experiments",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "bunx --bun react-router dev",
		"start": "react-router-serve ./build/server/index.js",
		"lint": "bunx --bun eslint . --cache --cache-location ./node_modules/.cache/eslint",
		"format": "bunx --bun prettier . --ignore-path .gitignore --write --cache --cache-location ./node_modules/.cache/prettier",
		"typecheck": "bunx --bun react-router typegen && tsc",
		"fun": "bun format && bun typecheck && bun lint",
		"cron": "bun run cron/index.ts"
	},
